supervisor="supervise-daemon"
CFG_NAME=$(echo ${RC_SVCNAME} | cut -d '.' -f 2)
command="/usr/bin/ssh-remote /etc/conf.d/ssh-remote.${CFG_NAME}"

depend() {
	use logger dns net
}
